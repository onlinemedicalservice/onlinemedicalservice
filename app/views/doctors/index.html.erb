<h1>Listing doctors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Dob</th>
      <th>Gender</th>
      <th>Nationality</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Pincode</th>
      <th>Mobile number</th>
      <th>Phone number</th>
      <th>Blood group</th>
      <th>Qualification</th>
      <th>Email</th>
      <th>User</th>
      <th>Admin</th>
      <th>Specialization</th>
      <th>Designation</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @doctors.each do |doctor| %>
      <tr>
        <td><%= doctor.name %></td>
        <td><%= doctor.dob %></td>
        <td><%= doctor.gender %></td>
        <td><%= doctor.nationality %></td>
        <td><%= doctor.address %></td>
        <td><%= doctor.city %></td>
        <td><%= doctor.state %></td>
        <td><%= doctor.country %></td>
        <td><%= doctor.pincode %></td>
        <td><%= doctor.mobile_number %></td>
        <td><%= doctor.phone_number %></td>
        <td><%= doctor.blood_group %></td>
        <td><%= doctor.qualification %></td>
        <td><%= doctor.email %></td>
        <td><%= doctor.user_id %></td>
        <td><%= doctor.admin_id %></td>
        <td><%= doctor.specialization %></td>
        <td><%= doctor.designation %></td>
        <td><%= link_to 'Show', doctor %></td>
        <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
        <td><%= link_to 'Destroy', doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doctor', new_doctor_path %>
